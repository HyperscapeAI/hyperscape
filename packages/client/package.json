  "keywords": [
    "hyperscape",
    "3d",
    "components",
    "virtual-worlds",
    "metaverse",
    "three.js",
    "webgl",
    "real-time"
  ],
  "scripts": {
    "dev": "vite --host --port 3333",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint src --max-warnings 22100",
    "deploy": "node scripts/deploy-cloudflare.mjs",
    "deploy:prod": "node scripts/deploy-cloudflare.mjs --production",
    "cap:sync": "cap sync",
    "cap:sync:ios": "cap sync ios",
    "cap:sync:android": "cap sync android",
    "ios": "bun run build && cap sync ios && cap open ios",
    "ios:dev": "cap sync ios && cap open ios",
    "ios:build": "bun run build && cap sync ios && cap copy ios",
    "android": "bun run build && cap sync android && cap open android",
    "android:dev": "cap sync android && cap open android",
    "android:build": "bun run build && cap sync android && cap copy android"
  },
  "files": [
    "dist/",
    "README.md",
    "LICENSE"
  ],
